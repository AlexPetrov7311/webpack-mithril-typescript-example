{
  "name": "webpack-mithril-typescript-example",
  "description": "An example of a project that uses Mithril and TypeScript, build with Webpack",
  "author": "Matthew Palmer <mspalmer91@gmail.com>",
  "version": "0.0.0",
  "license": "MIT",
  "keywords": [
    "webpack",
    "mithril",
    "mithriljs",
    "typescript",
    "example"
  ],
  "config": {
    "browserSupport": [
      "last 2 versions",
      "ie 10"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "mithril": "^0.2.5"
  },
  "devDependencies": {
    "@types/mithril": "0.0.30",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "path": "^0.12.7",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016"
    ]
  }
}
