  },
  "devDependencies": {
    "@types/mithril": "0.0.30",
    "autoprefixer": "^6.4.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "css-loader": "^0.24.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "html-webpack-plugin": "^2.22.0",
    "node-sass": "^3.8.0",
    "path": "^0.12.7",
    "postcss-loader": "^0.10.1",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0",
    "webpack-merge": "^0.14.1",
    "webpack-validator": "^2.2.7"
  },
  "babel": {
    "presets": [
      "es2015",
      "es2016"
    ]
  }
